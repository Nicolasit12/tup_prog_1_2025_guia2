Algoritmo NumerosA
	Definir numero1 Como Real;
	Escribir 'ingresar el numero';
	Leer numero1;
	Si numero1>0 Entonces
		Escribir 'el numero es positivo';
	SiNo
		Si numero1<0 Entonces
			Escribir 'el numero es negativo';
		SiNo
			Escribir 'el numero es cero';
		FinSi
	FinSi
	
FinAlgoritmo
